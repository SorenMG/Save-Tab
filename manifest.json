{
    "name": "SaveTab",
    "version": "1.4",
    "manifest_version": 2,

    "description": "Save multiple Chrome sessions for later, with just a click of a button.",
    
    "author": "Soren Gade",
    
    "permissions": [
        "tabs",
        "storage"
    ],


    "icons": {
        "16": "icon/icon_16.png",
        "48": "icon/icon_48.png",
        "128": "icon/icon_128.png"
    },
    
    "browser_action": {  
        "default_popup": "index.html",
        "default_title": "SaveTab"
    }
}